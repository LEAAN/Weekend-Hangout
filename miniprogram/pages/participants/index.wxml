<block wx:for="{{userResult}}" wx:key="*this">
	<view class="weui_cell">
		<view class="weui_cell_hd">
			<image src="{{item.icon}}" style="border-radius:50%"></image>
		</view>
		<view class="weui_cell_bd">
      <view class="weui_cell_bd_p"> {{item.alias}} </view>
		</view>
		<view class="with_arrow" bindtap="getFriendInfo"></view>
	</view>
</block>
